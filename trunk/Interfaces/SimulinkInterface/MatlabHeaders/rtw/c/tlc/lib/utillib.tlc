  %if !SLibSingleTasking()
    %with ::CompiledModel
      %assign tid01Eq = ISEQUAL(SolverType, "FixedStep") && FixedStepOpts.TID01EQ
      %assign numSyncTasks = (NumSynchronousSampleTimes - tid01Eq)
