    %setcommandswitch "-v1"
  %endif
  %assign errorMessage = FEVAL("rtwprivate", "tlcreporterror", errorId)
  %exit %<errorMessage>
%endfunction

%% Function: SLibReportErrorWithIdAndArgs =================================
